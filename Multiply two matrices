#include <iostream>
#include<math.h>

using namespace std;

int main() {
  int a[10][10], b[10][10], c[10][10];
  int result = 0;
  int i, j, p, q, r, s, k;
  
    cout << "Enter rows and columns of first matrix: " << endl;
    cin >> p >> q;
    
    cout << "Enter first matrix: " << endl;
    for (int i=0; i < p; i++) {
        for (int j=0; j < q; j++) {
            cin >> a[i][j];
        }
    }

  cout << "Enter rows and columns of second matrix: " << endl;
  cin >> r >> s ;
  
  cout << "Enter second matrix: " << endl;
  for (int i=0; i < r; i++) {
        for (int j=0; j < s; j++) {
            cin >> b[j][j];
        }
   }
   
  if (q == r) {
      
  cout << "The new matrix is: " << endl;
  for (int i=0; i < p; i++) {
        for (j=0; j < s; j++) {
            c[i][j] = 0;
            
            for (k=0; k < q; k++) {
                c[i][j] += a[i][k] * b[k][j];
            }
            cout << c[i][j] << " ";
        }
        cout << "\n";
    }
  }
  else 
    cout << "Invalid syntax";
}
