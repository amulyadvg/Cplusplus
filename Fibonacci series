#include <iostream>
#include<math.h>
#include<string>
#include<cstdlib>
using namespace std;

int main() {
    int n;
    int n1 = 0;
    int n2 = 1;
    int n3 = 0;
    cout << "Enter number of elements: " << endl;
    cin >> n;
    
    cout << n1 << " " << n2 << " ";
    
    for (int i=2; i < n; i++) {
        n3 = n1 + n2;
        cout << n3 << " ";
        
        n1 = n2;
        n2 = n3;
    }
return 0;
}
